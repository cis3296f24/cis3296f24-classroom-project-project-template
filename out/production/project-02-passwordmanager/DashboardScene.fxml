<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="418.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DashboardController">
   <children>
      <Label layoutX="232.0" layoutY="7.0" prefHeight="29.0" prefWidth="137.0" text="Dashboard">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ButtonBar layoutX="386.0" layoutY="-1.0" prefHeight="45.0" prefWidth="212.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#settingButtonClicked" text="Settings" />
        </buttons>
      </ButtonBar>
      <ScrollPane layoutX="-2.0" layoutY="38.0" prefHeight="600.0" prefWidth="600.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="596.0" prefWidth="599.0">
               <children>
                  <VBox alignment="TOP_CENTER" layoutX="86.0" layoutY="280.0" prefHeight="86.0" prefWidth="435.0">
                     <children>
                        <Label text="Label" />
                        <Label alignment="CENTER" prefHeight="17.0" prefWidth="430.0" text="ResultLabel" />
                        <TextField prefHeight="25.0" prefWidth="389.0" />
                        <Button alignment="TOP_CENTER" mnemonicParsing="false" text="Button" />
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" layoutX="84.0" layoutY="187.0" prefHeight="86.0" prefWidth="439.0">
                     <children>
                        <Label text="Password Strength Checker"/>
                        <Label fx:id="passwordStrengthCheckerLabel" alignment="CENTER" prefHeight="17.0" prefWidth="439.0" text="PasswordStrengthResult" />
                        <TextField fx:id="passwordInput" prefHeight="25.0" prefWidth="389.0"/>
                        <Button alignment="TOP_CENTER" mnemonicParsing="false" onAction="#checkPasswordStrength" text="Check Password Strength" />
                     </children>
                  </VBox>
                  <TableView layoutX="32.0" prefHeight="161.0" prefWidth="540.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="Credentials" />
                    </columns>
                  </TableView>
                  <VBox alignment="TOP_CENTER" layoutX="87.0" layoutY="390.0" prefHeight="86.0" prefWidth="432.0">
                     <children>
                        <Label text="Encryption TEST interface" />
                        <Label fx:id="encryptionTestLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="435.0" text="Encryption results here" />
                        <TextField fx:id="plainTextData" prefHeight="25.0" prefWidth="575.0" />
                        <Button alignment="TOP_CENTER" mnemonicParsing="false" onAction="#encryptionTestButtonClicked" text="Button" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
   </children>
</AnchorPane>
